<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<welcome-file-list>
		<welcome-file>student-registration.jsp</welcome-file>
	</welcome-file-list>
	<!-- Student Insert Controller configuration -->
	<servlet>
		<servlet-class>com.ibm.servlet_simple_demo_project.controller.StudentInsertController</servlet-class>
		<servlet-name>register</servlet-name>
	</servlet>
	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/studentRegister</url-pattern>
	</servlet-mapping>

	<!-- Student Delete Controller configuration -->
	<servlet>
		<servlet-class>com.ibm.servlet_simple_demo_project.controller.StudentDeleteController</servlet-class>
		<servlet-name>del</servlet-name>
	</servlet>
	<servlet-mapping>
		<servlet-name>del</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>



</web-app>
